<div class="section">
  <div class="container">
    <h1>Project Classes</h1>
    <table class="table">
      <tr>
        <th>ID</th>
      </tr>
      <% @projectclasses.each do |projectclass| %>
        <tr>
          <td><%= link_to projectclass.id, project_class_path(@project, projectclass) %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
